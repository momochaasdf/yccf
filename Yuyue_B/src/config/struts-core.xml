	<package name="ComUser.login" extends="struts" namespace="/">
		<action name="login" class="com.deying.core.action.LoginAction" method="login">
			<result name="success" type="redirect">/loginSuccess.jsp</result>
			<result name="successIntegralShop" type="redirect">/loginSuccessIntegralShop.jsp</result>
			<result name="input" type="redirect" >/cooperation_loadCooperationShop.action?msg=${msg}</result>
		</action>
		<action name="exit" class="com.deying.core.action.LoginAction" method="exit">
			<result name="input" type="redirect" >/cooperation_loadCooperationShop.action?msg=${msg}</result>
		</action>
