<struts>
	<package name="core.customer.loan" extends="struts" namespace="/core/customer/loan">
		<action name="ComM_*" class="com.deying.core.action.LoanCustomerAction" method="{1}">
			<result name="list">/WEB-INF/pages/core/customer_loan_{1}.jsp</result>
			<result name="input">/WEB-INF/pages/core/customer_loan_list.jsp</result>
		</action>
		<action name="ComC_*" class="com.deying.core.action.LoanCustomerAction" method="{1}">
			<result name="add">/WEB-INF/pages/core/customer_loan_add.jsp</result>
			<result name="list">/WEB-INF/pages/core/customer_loan_list.jsp</result>
			<result name="input">/WEB-INF/pages/core/customer_loan_add.jsp</result>
		</action>
		<action name="ComU_*" class="com.deying.core.action.LoanCustomerAction" method="{1}">
			<result name="edit">/WEB-INF/pages/core/customer_loan_{1}.jsp</result>
			<result name="list">/WEB-INF/pages/core/customer_loan_list.jsp</result>
			<result name="input">/WEB-INF/pages/core/customer_loan_edit.jsp</result>
		</action>
		<action name="ComD_*" class="com.deying.core.action.LoanCustomerAction" method="{1}">
			<result name="list">/WEB-INF/pages/core/customer_loan_list.jsp</result>
			<result name="input">/WEB-INF/pages/core/customer_loan_list.jsp</result>
		</action>
		<action name="ComR_*" class="com.deying.core.action.LoanCustomerAction" method="{1}">
			<result name="load">/WEB-INF/pages/core/customer_loan_{1}.jsp</result>
			<result name="input">/WEB-INF/pages/core/customer_loan_load.jsp</result>
		</action>
	</package>
