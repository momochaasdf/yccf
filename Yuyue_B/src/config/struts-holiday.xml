<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="holiday" extends="struts" namespace="/holiday">
		<action name="holiday_*" class="com.deying.core.action.HolidayAction" method="{1}">
			<result name="input">/WEB-INF/pages/holiday/holiday_list.jsp</result>
		</action>
	</package>
	<package name="holidayJ" extends="json" namespace="/holiday">
		<action name="holidayJ_*" class="com.deying.core.action.HolidayAction" method="{1}" />
	</package>
</struts>